{
  "buildCommand": "cd frontend && npm install --legacy-peer-deps && npm run build",
  "outputDirectory": "frontend/build",
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/((?!robots.txt|llms.txt|sitemap.xml|favicon.ico|_next/|static/|api/).*)",
      "destination": "/index.html"
    }
  ]
}
